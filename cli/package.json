{
  "name": "mongo-seeding-cli",
  "description": "The ultimate command line tool for populating your MongoDB database.",
  "version": "3.0.0",
  "license": "MIT",
  "keywords": [
    "mongo",
    "mongodb",
    "seed",
    "db-seed",
    "database",
    "seeding",
    "db",
    "cli",
    "command-line",
    "command-line-interface",
    "command",
    "line",
    "interface"
  ],
  "tags": [
    "mongo",
    "mongodb",
    "seed",
    "db-seed",
    "database",
    "seeding",
    "db",
    "cli",
    "command-line",
    "command-line-interface",
    "command",
    "line",
    "interface"
  ],
  "author": {
    "name": "Pawe≈Ç Kosiec",
    "email": "pawel@kosiec.net",
    "url": "https://www.kosiec.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pkosiec/mongo-seeding"
  },
  "main": "dist/index.js",
  "files": [
    "dist/",
    "bin/"
  ],
  "bin": {
    "seed": "bin/seed.js"
  },
  "scripts": {
    "build": "npm run cleanup; tsc",
    "cleanup": "rm -rf ./dist",
    "preversion": "npm run build",
    "test": "jest -i",
    "test:unit": "jest unit",
    "test:integration": "jest integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage"
  },
  "dependencies": {
    "command-line-args": "^5.0.0",
    "command-line-usage": "^5.0.0",
    "extend": "^3.0.2",
    "mongo-seeding": "^3.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/extend": "^3.0.0",
    "@types/jest": "^23.3.9",
    "@types/mongodb": "^3.1.14",
    "@types/node": "^10.12.2",
    "jest": "^23.6.0",
    "mongodb": "^3.1.8",
    "ts-jest": "^23.10.4"
  },
  "jest": {
    "verbose": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/integration/_importdata/"
    ],
    "testRegex": "(/test/[^_][a-zA-Z0-9/]*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
