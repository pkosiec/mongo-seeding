{
  "name": "docker-image-tester",
  "description": "Tester app for Mongo Seeding Docker Image",
  "version": "3.7.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Pawe≈Ç Kosiec",
    "email": "pawel@kosiec.net",
    "url": "https://kosiec.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pkosiec/mongo-seeding"
  },
  "scripts": {
    "start": "npm test",
    "test": "jest -i"
  },
  "dependencies": {
    "bson": "^4.2.2",
    "jest": "^27.0.4",
    "mongodb": "^3.6.3"
  },
  "jest": {
    "verbose": false,
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "testRegex": "(/[^_][a-zA-Z0-9/]*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
