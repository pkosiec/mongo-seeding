{
  "name": "docker-image-tester",
  "description": "Tester app for Mongo Seeding Docker Image",
  "homepage": "https://mongo-seeding.kosiec.dev",
  "version": "4.0.1",
  "license": "MIT",
  "private": true,
  "maintainers": [
    {
      "name": "Pawe≈Ç Kosiec",
      "email": "pawel@kosiec.dev",
      "url": "https://kosiec.dev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pkosiec/mongo-seeding.git"
  },
  "bugs": {
    "url": "https://github.com/pkosiec/mongo-seeding/issues"
  },
  "scripts": {
    "start": "npm test",
    "test": "jest -i"
  },
  "dependencies": {
    "bson": "^6.1.0",
    "jest": "^29.7.0",
    "mongodb": "^6.1.0"
  },
  "jest": {
    "verbose": false,
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "testRegex": "(/[^_][a-zA-Z0-9/]*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
